{
  "name": "@nathan3boss/tsconfig",
  "description": "My personal set of tsconfig.json files",
  "version": "0.0.3",
  "files": [
    "skryp/*"
  ],
  "keywords": [
    "tsconfig",
    "nathan3boss"
  ],
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "prettier": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@3.5.0",
  "installConfig": {
    "hoistingLimits": "none"
  }
}
